<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean id="mrDemStorePanel" class="org.geoserver.web.data.resource.DataStorePanelInfo">
    <property name="id" value="mrDem" />
